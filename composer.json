{
    "name": "silvadan/molezinha-porto-laravel",
    "description": "Small packages to setup some basics for Porto usage",
    "require": {
        "prettus/l5-repository": "^2.6",
        "optimus/api-consumer": "^0.2.3",
        "optimus/heimdal": "^1.5",
        "barryvdh/laravel-cors": "^0.11"
    },
    "authors": [
        {
            "name": "Daniel S.F Silva",
            "email": "daniel.felix05@hotmail.com"
        }
    ],
    "license": "MIT",
    "keywords": [
        "molezinha",
        "Porto",
        "Porto SAP"
    ],
    "autoload": {
        "psr-4": {
            "Molezinha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Molezinha\\Providers\\MolezinhaServiceProvider"
            ],
            "aliases": {
                "Molezinha": "Molezinha\\Core\\Facades\\Molezinha"
            }
        }
    }
}
